.hide{transition: height 1s 0s, width  1s 0s, font-size 0s 1s, color 0s 1s;}
.show{transition: height 1s 0s, width  1s 0s, font-size 0s 0s, color 0s 0s;}
#H_contact{width:140px;height:140px;z-index:1;font-weight: bold;font-size: 12px;color: white;background-color: #E24220;position: fixed;border-radius: 20px;font-weight: 700;margin-left: 35px;margin-top: 15px;}
#H_contact>ul{list-style-type: none;padding: 0px 10px 0px 10px;position: relative;margin-bottom: 3px; margin-top: 31px; text-align: center}
#H_contact p{margin-bottom: 7px;padding-left: 05px; padding-right: 05px}
#H_contact>ul>li{display: block;height: 20px;}
#H_hide{cursor: pointer;transition: 1s;font-size: 35px;position: absolute;right: 0px;top: 0px;line-height: 0px;padding: 17px 12px;padding-top: 8px;width: 45px;text-align: center;border-bottom-left-radius: 20px;background-color: white;color: #E24220;border-top-right-radius: 20px;border: 1px solid #E24220}
#Img-contact{line-height: 0px;position: absolute;background-color: #E24220;padding: 0px;border-radius: 100%;margin-left:24.5px;margin-top:12px;box-sizing: border-box;z-index: 2;height: 23px;width: 23px}
#img_{width: 23px;height:23px}
.Img-contact{z-index: -100;position: absolute;background-color: #E24220;padding: 15px;border-radius: 100%;margin-left:19.5px;margin-top: 6px;width: 34px;height:34px;animation-name: contact;opacity: 0.6;
    animation-duration: 1s; animation-iteration-count: infinite;}
.Img-contact2{z-index: -200;position: absolute;background-color: #E24220;padding: 15px;border-radius: 100%;margin-left:14px;margin-top: 0px;width: 45px;height:45px;;animation-name: contact;opacity: 0.3;
    animation-duration: 1s; animation-iteration-count: infinite;}
@keyframes contact {
    0%{transform: scale(0.8,0.8);}
    100%{transform: scale(1.05,1.05);}