#myBtn {
	display:none;
    position: fixed;
    bottom: 20px;
    right: 30px;
    z-index: 99;
    border: none;
    outline: none;
    color: white;
    cursor: pointer;
    background:#46495E;
    width:40px;
    height:40px;
    text-align:center;
    font-size:16px;
    padding-top:6px;
    border-radius:5px;
}

#myBtn:hover {
    background-color: #555;
}
@media screen and (max-width:1199px) {
    .centerdiv{width: 100%}
}
@media screen and (max-width:699px) {
    .footercl{width: 50%}
}
@media screen and (max-width:419px) {
    .footercl{width: 100%}}