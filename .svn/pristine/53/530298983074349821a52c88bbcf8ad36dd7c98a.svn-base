.introsection2{width:80%; max-width:1080px; margin-left: auto; margin-right: auto; padding-top: 30px;}
.Tn0spec{width: 100%; text-align: center; padding-top: 20px; background: #f0f0f0;overflow:hidden;position:relative}
.Tn0smallspec{width: 80%; max-width: 1080px;margin-left: auto; margin-right:  auto;position:relative;font-weight:bold;}
.Tn0warpspec{width: 23%; float: left; background: inherit;text-align: center;padding:20px; transition:all 0.5s; cursor:pointer; margin-bottom:30px; border: 1px dashed #EE4925; margin-right: 1%; margin-left: 1%}
.Tn0Iconspec{height:70px; width: 70px;margin-left: auto; margin-right:  auto; background: #FFFFFF;transition:all 1s; border:solid 1px #909090}
.Tn0warpspec:hover .Tn0Iconspec{background:#EE4925;border-radius: 35px; transition-duration:0.4s}
.Tn0warpspec:hover .Tn0specir{color:#FFFFFF}
.Tn0specp{font-size: 14px; color: #717171}
.Tn0specir{padding-top: 20px; padding-bottom: 20px; color: #EE4925; font-size: 30px}
.Tn0More{width: 150px; height: 40px; border-radius: 360px; border: 1px solid #d1cfcf; font-size: 13px; margin-left: auto; margin-right: auto; text-align: left; padding-top: 9px; padding-left: 15px; background: white}
.Tn0Morecir{height: 30px; width: 30px; border-radius: 360px; border: 1px solid #d1cfcf; float: right; margin-top: -5px; margin-right: 5px; background:#E24220 ; color: white; text-align: center; font-size: 18px }
.Tn0More:hover .Tn0Morecir{color:#E24220; background: #FFFFFF ;}
.Tn0warpcustomer{width: 100%; border-bottom: 5px solid #FFFFFF; padding-top: 20px; padding-bottom: 20px; background-position: center; background-size: 100%}
@media screen and (max-width:1199px) {


    .Tn0warpspec{width: 50%}
}
@media screen and (max-width:699px) {


    .Tn0warpspec{width: 100%}
}
