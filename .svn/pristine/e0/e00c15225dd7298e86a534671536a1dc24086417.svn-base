	<script src="Swiper-3.4.2/dist/js/swiper.min.js"></script>
	<script>
		function Swiper_update(slidesPerView) {

			//Update Swiper's slidesPerView
			var swiper = new Swiper('.swiper-container', {
				slidesPerView: slidesPerView,
				spaceBetween: 0,
				slidesPerGroup: 1,
				loop: false,
				loopFillGroupWithBlank: true,
				pagination: {
					el: '.swiper-pagination',
					clickable: true,
				},
				nextButton: '.left',
				prevButton: '.right',
			});


			//Update position of navigation in Swiper
			if (slidesPerView == 5) {
				editCSS("40px");
			} else if (slidesPerView == 3) {
				editCSS("15px");
			} else if (slidesPerView == 1) {
				editCSS("0px");
			}

		}

		function editCSS(pos) { //Update position of navigation in Swiper
			$(".left").css({
				'left': pos,
			});
			$(".right").css({
				'right': pos,
			});
		}

		$(function() { //Set swiper's slidePerView when loading page
			if ($('body').innerWidth() >= 1100) {
				Swiper_update(5);
			} else if ($('body').innerWidth() > 700 && $('body').innerWidth() < 1100) {
				Swiper_update(3);
			} else if ($('body').innerWidth() <= 700) {
				Swiper_update(1);
			}
		})

		$(window).resize(function() { //Set swiper's slidePerView when resize window
			if ($('body').innerWidth() >= 1100) {
				Swiper_update(5);
			} else if ($('body').innerWidth() > 700 && $('body').innerWidth() < 1100) {
				Swiper_update(3);
			} else if ($('body').innerWidth() <= 700) {
				Swiper_update(1);
			}
		})

	</script>
