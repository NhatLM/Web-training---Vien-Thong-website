.footercl{padding-left:10px;padding-right: 40px; padding-top: 10px; padding-bottom: 30px; width:20%;float: left; text-align: left}
.footerc2{padding-left:10px;padding-right: 50px; width:50%; text-align: right;float: right;font-size: 17px}
