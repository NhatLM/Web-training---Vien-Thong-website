$(function () {
    $(".content-left").on("click",function(){
       var child=$(this).siblings("div[class~='content-right']").children("i");
        if(child.hasClass("fa fa-angle-double-right")){
            child.removeClass("fa fa-angle-double-right");
            child.addClass("fa fa-angle-double-down");
        }
        // else{
        //     child.removeClass("fa fa-angle-double-down");
        //     child.addClass("fa fa-angle-double-right");
        // }

    })
})
