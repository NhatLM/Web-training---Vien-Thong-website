.warpfooter{ background-position: center; background-size: 100%; width: 100%; padding-top: 60px; padding-bottom: 40px; line-height: 30px;z}
.footer{width: 80%; max-width: 1080px}
.footerlogo{width: 18%; padding: 20px;float: left; border-right: 1px solid #FFFFFF}
.footerif{width: 60% ; margin-left: 1%; float: left; padding-left: 10px; padding-right: 10px}
.footertd{color: #FFFFFF; font-size: 13px}
.footera{color:#E24220 }
.footersa{color:white; text-decoration: none }
.footersa:hover {color:#E24220; text-decoration: none }
.info{width: 50%; padding-left: 10px; padding: 10px; float: left; font-size: 12px; color: #C9C9C9}
.footeri{color:#E24220}
.footerlink{width: 20%; margin-left: 1%; float: left}
.footercir{width: 35px; height: 35px; border-radius: 360px; border: 1px solid white;margin-left: 5px; float: left; font-size: 18px; padding-top: 02px; padding-left: 6.5px}
.copyright{float:left;width: 55px; background: #2e2e2e; border-bottom: 5px solid #E24220; text-align: center; font-size: 14px; color: white;width: 100%; padding-top: 14.5px; padding-bottom: 14.5px}
#myBtn {
    display:none;
    position: fixed;
    bottom: 20px;
    right: 30px;
    z-index: 99;
    border:none;
    outline: none;
    color: #FFFFFF;
    cursor: pointer;
    background:rgb(238, 73, 37);
    width:40px;
    height:40px;
    text-align:center;
    font-size:16px;
    padding-top:6px;
    border-radius:50%;
    box-shadow: 0.2px 0.2px 10px rgb(147, 63, 44);
}

#myBtn:hover {
    background-color: #555;
}
@media screen and (max-width:1199px) {
    .centerdiv{width: 100%}
}
@media screen and (max-width:1100px) {
    .footerlogo {width:20%;padding-left:0}
    .footerif {width:79%;}
    .footerlink{width: 100%;margin:0}
}
@media screen and (max-width:765px) {
    .footerlogo {
        width:40%;
        margin-left:30%;
        padding: 0;
        border-right:0;
        border-bottom:1px solid whitesmoke;
        margin-bottom:10px;
    }
    .footerlogo img {width:70%;margin-left: 15%}
    .footerif {width:100%;}
    .footerlink{width: 100%;margin:0}
}

@media screen and (max-width:699px) {
    .footercl{width: 50%}
    .info {width:100%}
}
@media screen and (max-width:419px) {
    .footercl{width: 100%}}